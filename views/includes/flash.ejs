
<%if(success && success.length){%>
    <div class="alert alert-success alert-dismissible fade show" role="alert" style="width: 60%; margin: auto; margin-top: 8px; font-weight: bold; text-align: center;">
        <%=success%> !
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
<%}%>